using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Ex15
{

    /* Questão 15: Escreva um programa que imprima todos os números inteiros de 0 a 50 */

    internal class Program
    {
        static void Main(string[] args)
        {
            for (int i = 0; i <= 50; i++)
            {
                Console.WriteLine(i);
            }
            Console.ReadKey();

        }
    }
}
